{
  "name": "react-chat",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "clean": "rm -rf build public/bundle.js",
    "build-backend": "babel server --out-dir build --presets=es2015",
    "build-frontend-production": "NODE_ENV=production webpack -p --config ./webpack.config.production",
    "build-both-production": "npm run build-backend && npm run build-frontend-production",
    "build-frontend-development": "NODE_ENV=development webpack -p",
    "build-both-development": "npm run build-backend && npm run build-frontend-development",
    "start-production": "cross-env NODE_ENV=production node ./build/app.js",
    "start-frontend-development": "webpack-dev-server",
    "start-backend-development": "cross-env NODE_ENV=development nodemon --exec babel-node --presets=es2015 ./server/app.js --watch server"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/brightparagon/react-chat.git"
  },
  "keywords": [
    "react.js",
    "node.js",
    "express.js",
    "webpack",
    "babel",
    "es6",
    "redux",
    "socket.io",
    "chat"
  ],
  "author": "brightparagon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brightparagon/react-node-boilerplate/issues"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "css-loader": "^0.28.1",
    "express": "^4.15.2",
    "method-override": "^2.3.8",
    "morgan": "^1.9.0",
    "path": "^0.12.7",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-google-login": "^2.11.2",
    "react-redux": "^5.0.4",
    "react-router-dom": "^4.2.2",
    "redux": "^3.6.0",
    "redux-saga": "^0.16.0",
    "reselect": "^3.0.1",
    "socket.io": "^2.0.1",
    "socket.io-client": "^2.0.3",
    "style-loader": "^0.17.0",
    "superagent": "^3.5.2",
    "url-loader": "^0.5.8"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "cross-env": "^5.0.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "file-loader": "^1.1.4",
    "html-webpack-plugin": "^2.30.1",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^3.1.1",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "webpack": "^3.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-dev-server": "^2.9.1"
  }
}
