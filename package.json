{
  "name": "react-node-boilerplate",
  "version": "1.0.0",
  "description": "Boilerplate for building apps with React.js, Node.js and Express.js",
  "main": "app.js",
  "scripts": {
    "clean": "rm -rf build public/bundle.js",
    "build": "babel server --out-dir build --presets=es2015",
    "webpack": "webpack",
    "start": "cross-env NODE_ENV=production node ./build/app.js",
    "development": "cross-env NODE_ENV=development nodemon --exec babel-node --presets=es2015 ./server/app.js --watch server"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brightparagon/react-node-boilerplate.git"
  },
  "babel": {
    "presets": [
      [
        "es2015",
        {
          "modules": false
        }
      ],
      "react",
      [
        "env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "react-hot-loader/babel"
    ]
  },
  "keywords": [
    "react.js",
    "node.js",
    "express.js",
    "webpack",
    "babel",
    "es6",
    "redux"
  ],
  "author": "brightparagon",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/brightparagon/react-node-boilerplate/issues"
  },
  "homepage": "https://github.com/brightparagon/react-node-boilerplate#readme",
  "dependencies": {
    "babel": "^6.23.0",
    "babel-core": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.3",
    "css-loader": "^0.28.1",
    "express": "^4.15.2",
    "file-loader": "^0.11.1",
    "method-override": "^2.3.8",
    "path": "^0.12.7",
    "react": "^15.5.4",
    "react-addons-update": "^15.5.2",
    "react-dom": "^15.5.4",
    "react-redux": "^5.0.4",
    "react-router": "^3.0.5",
    "redux": "^3.6.0",
    "redux-thunk": "^2.2.0",
    "style-loader": "^0.17.0",
    "superagent": "^3.5.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.5.1"
  },
  "devDependencies": {
    "babel-preset-env": "^1.4.0",
    "cross-env": "^5.0.0",
    "nodemon": "^1.11.0",
    "react-hot-loader": "^1.3.1",
    "uglify-js": "^2.8.24",
    "uglifyjs-webpack-plugin": "^0.4.3",
    "webpack-dev-server": "^2.4.5"
  }
}
